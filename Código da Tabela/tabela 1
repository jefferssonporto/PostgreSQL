crate table ALUNO (
	CODIGOALUNO int NOT NULL,
	NOME varchar(90) NOT NULL,
	SEXO char(1) NOT NULL,
	DTNASCIMENTO date (30) NOT NULL,
	EMAIL varchar(30) NULL,
	CONSTRAINT ALUNO_pk PRIMARY KEY (codigoALUNO));
